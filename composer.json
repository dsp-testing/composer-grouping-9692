{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "bugsnag/bugsnag-laravel": "^2.0",
    "dotweb/portal-common": "^11.2.0",
    "inertiajs/inertia-laravel": "v0.6.11",
    "laravel/framework": "^10.0",
    "laravel/helpers": "^1.1",
    "maatwebsite/excel": "^3.1.44",
    "nesbot/carbon": "^2.34",
    "symfony/psr-http-message-bridge": "^2.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.16",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.46",
    "larastan/larastan": "^2.9.9",
    "laravel/tinker": "^2.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.12.7",
    "spatie/ray": "^1.17"
  },
  "autoload": {
    "psr-4": {
      "DotwebCloudAdmin\\": "app/"
    },
    "files": [
      "app/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan package:discover"
    ],
    "post-install-cmd": [
      "php artisan vendor:publish --provider=\"DotwebCloud\\Common\\ServiceProvider\" --force"
    ],
    "php-cs-fixer:fix": "vendor/bin/php-cs-fixer fix",
    "php-cs-fixer:check": "vendor/bin/php-cs-fixer check --diff",
    "phpstan": "phpstan analyse --memory-limit=2048M",
    "checkup": [
      "@phpstan",
      "@php-cs-fixer:check"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:visma-verzuim/dwc-utils-portal.git"
    }
  ]
}
